  "scripts": {
    "prestart": "run-s merge",
    "start": "react-app-rewired start",
    "prebuild": "run-s merge",
    "build": "react-app-rewired --max_old_space_size=4096 build",
    "merge": "react3l translate merge -i src/ -o src/locales/ -p src/i18n/",
    "extract": "react3l translate extract -i src/ -o src/i18n/ -p src/i18n/",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,html,css,less,scss}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "modules": true
      },
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    },
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "rules": {
          "no-undef": "off"
        }
      }
    ],
    "ignorePatterns": [
      "src/**/*.test.{ts,tsx}"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
